{
  "name": "orlovkn/laravel-extention",
  "description": "Laravel package example",
  "license": "MIT",
  "authors": [
    {
      "name": "Konstantin Orlov",
      "email": "orlovkn.job@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/database": "^9.0",
    "illuminate/support": "^9.0",
    "illuminate/console": "^9.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "orchestra/testbench": "^7.6"
  },
  "autoload": {
    "psr-4": {
      "Orlovdev\\LaravelExtention\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Orlovdev\\LaravelExtention\\Test\\": "tests/"
    }
  },
  "scripts": {
    "test": [
      "vendor/bin/phpunit"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Orlov\\LaravelExtention\\Providers\\EloquentFilterServiceProvider"
      ]
    }
  }
}
